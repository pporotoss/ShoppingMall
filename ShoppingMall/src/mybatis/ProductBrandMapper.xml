<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ProductBrand">
  
  <insert id="insert" parameterType="ProductBrand">
  	insert into productbrand(productbrand_id, product_id, brand_id)
  	values(seq_productbrand.nextval, product_id=#{product_id}, brand_id=#{brand_id})
  </insert>
  
  <delete id="delete" parameterType="int">
  	delete from productbrand where product_id=#{product_id}
  </delete>
</mapper>