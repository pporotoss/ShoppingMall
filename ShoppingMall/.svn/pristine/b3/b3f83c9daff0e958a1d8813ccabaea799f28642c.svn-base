package com.shoppingmall.product.model;

public class ProductSize {
	private int productsize_id;
	private int product_id;
	private int panelsize_id;
	public int getProductsize_id() {
		return productsize_id;
	}
	public int getProduct_id() {
		return product_id;
	}
	public int getPanelsize_id() {
		return panelsize_id;
	}
	public void setProductsize_id(int productsize_id) {
		this.productsize_id = productsize_id;
	}
	public void setProduct_id(int product_id) {
		this.product_id = product_id;
	}
	public void setPanelsize_id(int panelsize_id) {
		this.panelsize_id = panelsize_id;
	}
	
}
